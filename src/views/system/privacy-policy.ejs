<%- contentFor('HeaderCss') %> <%- contentFor('title') %> Form Elements <%-
contentFor('breadcrumb') %>
<link
href="../../../public/libs/sweetalert2/sweetalert2.min.css"
rel="stylesheet"
type="text/css"
/>
<div class="col-sm-6">
  <div class="page-title-box">
    <h4 class="font-size-18">Add Products</h4>
    <ol class="breadcrumb mb-0">
      <li class="breadcrumb-item">
        <a href="javascript: void(0);">Veltrix</a>
      </li>
      <li class="breadcrumb-item"><a href="javascript: void(0);">Forms</a></li>
      <li class="breadcrumb-item active"></li>
    </ol>
  </div>
</div>

<%- contentFor('body') %>
<div class="row">
  <div class="col-12">
    <div class="card">
      <div class="card-body">
        <h4 class="card-title">Update Privacy Policy</h4>
        <hr>
        <div class="form-group row">
          <label for="privacy-policy" class="col-sm-2 col-form-label"
            >Privacy Policy:
            </label
          >
          <div class="col-sm-10">
            
          </div>
        </div>

       <div class="form-group-row">
        <form method="post">
            <textarea id="elm1" name="privacy-policy"></textarea>
          </form>
       </div>
       <hr>
       <h4>Update Terms Conditions</h4>
       <hr>
       <div class="form-group row">
        <label for="terms-conditions" class="col-sm-2 col-form-label"
          >Terms & Conditions:
          </label
        >
        <div class="col-sm-10">
          
        </div>
      </div>

     <div class="form-group-row">
      <form method="post">
          <textarea id="elm2" name="terms-conditions"></textarea>
        </form>
     </div>
        <hr>
          <form action="/update-privacy-policy" method="POST">
            <button
                type="submit"
                class="btn btn-primary waves-effect waves-light"
               id="update-contact-info">
               Update
            </button>
        </form>
        

      </div>
    </div>
  </div>
  <!-- end col -->
</div>
<!-- end row -->

<%- contentFor('FooterJs') %> <%- contentFor('BottomJs') %>

<!-- Sweet Alerts js -->
<script src="../../../public/libs/sweetalert2/sweetalert2.min.js"></script>

<!-- Sweet alert init js-->
<script src="../../../public/js/pages/sweet-alerts.init.js"></script>

<!--tinymce js-->
