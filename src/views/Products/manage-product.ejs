<%- contentFor('HeaderCss') %> <%- contentFor('title') %> Email Read <%-
contentFor('breadcrumb') %>
<div class="col-sm-6">
  <div class="page-title-box">
    <h4 class="font-size-18">Email Read</h4>
    <ol class="breadcrumb mb-0">
      <li class="breadcrumb-item">
        <a href="javascript: void(0);">Veltrix</a>
      </li>
      <li class="breadcrumb-item"><a href="javascript: void(0);">Email</a></li>
      <li class="breadcrumb-item active">Email Read</li>
    </ol>
  </div>
</div>

<%- contentFor('body') %>

<div class="row">
  <div class="col-12">
    <div class="card">
      <div class="card-body">
        <% if (data != null) { %>
        <table class="table table-hover mb-0">
          <thead>
            <tr>
              <th>ID</th>
              <th>Name</th>
              <th>Image</th>
              <th>Price</th>
              <th>Measurment</th>
              <th>Stock</th>
              <th>Availability</th>
              <th>Action</th>
            </tr>
          </thead>
          <tbody>
            <% data.forEach(result => { %>
            <!-- <tr><%= result.id %></tr>
            <tr><%= result.name %></tr>
            <tr><%= result.slug %></tr> -->
            <% }); %>
          </tbody>
        </table>
        <% } %>
      </div>
    </div>
  </div>
  <!-- end col -->
</div>

<%- contentFor('FooterJs') %> <%- contentFor('BottomJs') %>

<script src="../../../public/libs/sweetalert2/sweetalert2.min.js"></script>

<script src="../../../public/js/pages/sweet-alerts.init.js"></script>
