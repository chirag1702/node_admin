<%- contentFor('HeaderCss') %> <%- contentFor('title') %> Email Read <%-
contentFor('breadcrumb') %>

<!-- Responsive Table css -->
<link href="public/libs/admin-resources/rwd-table/rwd-table.min.css" rel="stylesheet" type="text/css"/>

<div class="col-sm-6">
    <div class="page-title-box">
        <h4 class="font-size-18">Email Read</h4>
        <ol class="breadcrumb mb-0">
            <li class="breadcrumb-item">
                <a href="javascript: void(0);">Veltrix</a>
            </li>
            <li class="breadcrumb-item"><a href="javascript: void(0);">Email</a></li>
            <li class="breadcrumb-item active">Transactions</li>
        </ol>
    </div>
</div>

<%- contentFor('body') %>

<div class="row">
    <div class="col-12">
        <div class="card">
            <div class="card-body">
                <h4>Transactions</h4>
                <table id="transaction" class="table table-hover">
                    <thead>
                    <tr>
                        <th data-priority="3">User Name</th>
                        <th data-priority="1">Order ID</th>
                        <th data-priority="3">Type</th>
                        <th data-priority="3">TXN ID</th>
                        <th data-priority="6">Amount</th>
                        <th data-priority="6">Status</th>
                        <th data-priority="6">Message</th>
                        <th data-priority="6">Transaction Date</th>
                    </tr>
                    </thead>
                    <tbody>
                    <% if (data != null) { %>
                        <% data.forEach(result => { %>
                            <tr>
                                <td><%= result.user_id %></td>
                                <td><%= result.order_id %></td>
                                <td><%= result.type %></td>
                                <td><%= result.txn_id %></td>
                                <td><%= result.amount %></td>
                                <td><%= result.status %></td>
                                <td><%= result.message %></td>
                                <td><%= result.transaction_date %></td>
                            </tr>
                        <% }) %>
                    <% } %>
                    </tbody>
                </table>

            </div>
        </div>
    </div>
    <!-- end col -->
</div>

<%- contentFor('FooterJs') %> <%- contentFor('BottomJs') %>

<script src="public/libs/sweetalert2/sweetalert2.min.js"></script>

<script src="public/js/pages/sweet-alerts.init.js"></script>

<!-- Responsive Table js -->
<script src="public/libs/admin-resources/rwd-table/rwd-table.min.js"></script>

<!-- Init js -->
<script src="public/js/pages/table-responsive.init.js"></script>

